{
  "name": "@ap0nia/eden-svelte-query",
  "version": "1.7.1-canary.0",
  "description": "elysia.js eden and svelte-query integration",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ap0nia/eden-query",
    "directory": "packages/eden-svelte-query"
  },
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly --declaration",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts",
      "require": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/index.ts"
      ],
      "./*": [
        "./src/*.ts"
      ]
    }
  },
  "dependencies": {
    "@ap0nia/eden": "workspace:^",
    "@tanstack/svelte-query": "^5.51.15",
    "elysia": "^1.2.15"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.7",
    "msw": "^2.4.4",
    "svelte": "^5.25.3",
    "tsup": "^8.2.4",
    "typescript": "^5.6.3",
    "vite": "^6.2.3"
  },
  "peerDependencies": {
    "@tanstack/svelte-query": "^5.51.15",
    "elysia": "^1.2.15"
  },
  "publishConfig": {
    "access": "public"
  }
}
