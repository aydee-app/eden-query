{
  "name": "@ap0nia/eden",
  "version": "1.0.0-next.3",
  "description": "Fully type-safe Elysia.js client",
  "license": "MIT",
  "scripts": {
    "build": "rslib build",
    "test": "ATTEST_skipTypes=1 vitest",
    "test:types": "vitest test-d"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ap0nia/eden-query",
    "directory": "packages/eden2"
  },
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    "./prod": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "node": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./plugins-prod": {
      "types": "./src/plugins/index.ts",
      "import": "./src/plugins/index.ts",
      "node": "./src/plugins/index.ts",
      "require": "./src/plugins/index.ts",
      "default": "./src/plugins/index.ts"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "node": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins.js",
      "node": "./dist/plugins.js",
      "require": "./dist/plugins.cjs",
      "default": "./dist/plugins.js"
    }
  },
  "devDependencies": {
    "@ark/attest": "^0.45.6",
    "@rslib/core": "^0.6.2",
    "@trpc/client": "^11.0.1",
    "@trpc/server": "^11.0.1",
    "@types/event-source-polyfill": "^1.0.5",
    "@types/jsdom": "^21.1.7",
    "devalue": "^5.1.1",
    "event-source-polyfill": "^1.0.31",
    "jsdom": "^26.0.0",
    "query-string": "^9.1.1",
    "rslib": "^0.0.10",
    "superjson": "^2.2.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "elysia": "^1.2.25"
  },
  "publishConfig": {
    "access": "public"
  }
}
